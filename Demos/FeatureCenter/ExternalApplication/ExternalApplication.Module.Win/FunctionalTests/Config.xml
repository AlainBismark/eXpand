<?xml version="1.0" encoding="utf-8" ?>
<Options xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" TestRunTimeLimit="3"  DefaultTimeout="30">
	<Applications>
		<!-- Win -->
		<Application
			Name="ExternalApplication.Win"
			FileName="[WinAppBin]\ExternalApplication.Win.exe"
			AdapterAssemblyName="[WinAdapterAssemblyName]"
			CommunicationPort="4100"/>
		<!-- Web -->
		<Application
			Name="ExternalApplicationWeb"
			Url="http://localhost:5000"
			PhysicalPath="[WebAppBin]"
			AdapterAssemblyName="[WebAdapterAssemblyName]" UseIISExpress="true" WebBrowserType="Stdandalone"/>
		<!-- For IIS -->
		<!--Application
			Name="ExternalApplicationWeb"
			Url="http://localhost/ExternalApplicationWeb/Default.aspx"
			AdapterAssemblyName="[WebAdapterAssemblyName]" UseIISExpress="true" /-->
	</Applications>
	<TestDatabases>
		<Database xsi:type="TestMSSQLDatabase" Server=".\SQLEXPRESS" DBName="ExternalApplicationEasyTest"/>
	</TestDatabases>

	<Aliases>
		<Alias Name="DomainUserName" Value="%USERDOMAIN%\%USERNAME%" />
		<Alias Name="WinAdapterAssemblyName" Value="Xpand.ExpressApp.EasyTest.WinAdapter" />
		<Alias Name="WebAdapterAssemblyName" Value="Xpand.ExpressApp.EasyTest.WebAdapter" />
		<Alias Name="WinAppBin" Value="[ConfigPath]\..\..\..\FeatureCenter.Win\bin\EasyTest\" />
		<Alias Name="WebAppBin" Value="[ConfigPath]\..\..\ExternalApplicationWeb" />
	</Aliases>
</Options>
