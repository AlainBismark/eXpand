<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <ProductVersion>9.0.30729</ProductVersion>
    <TargetFramework>net461</TargetFramework>
    <AssemblyOriginatorKeyFile>..\Xpand.key\xpand.snk</AssemblyOriginatorKeyFile>
    <SignAssembly>true</SignAssembly>
    <OutputPath>..\..\Xpand.Dll\</OutputPath>
    <LangVersion>latest</LangVersion>
    <DebugSymbols>true</DebugSymbols>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <DebugType>pdbonly</DebugType>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="System.Data.DataSetExtensions" Version="4.*" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="fasterflect" Version="2.1.3" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="DevExpress.Data.v19.1">
      <HintPath>..\..\Xpand.Dll\DevExpress.Data.v19.1.dll</HintPath>
      <Private>False</Private>
      <SpecificVersion>False</SpecificVersion>
    </Reference>
    <Reference Include="DevExpress.Xpo.v19.1">
      <HintPath>..\..\Xpand.Dll\DevExpress.Xpo.v19.1.dll</HintPath>
    </Reference>
    <Reference Include="System.ServiceModel" />
    <Reference Include="Xpand.Utils">
      <HintPath>..\..\Xpand.Dll\Xpand.Utils.dll</HintPath>
      <SpecificVersion>False</SpecificVersion>
    </Reference>
    <Reference Include="System.configuration" />
    <Reference Include="System.Web" />
  </ItemGroup>
  <ItemGroup>
    <Compile Update="Collections\XpandXPCollection.cs">
      <SubType>Component</SubType>
    </Compile>
    <Compile Update="XpandUnitOfWork.cs">
      <SubType>Component</SubType>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <Compile Remove="DBObject.cs" />
    <Compile Remove="testexpand.cs" />
    <Compile Remove="XpandXPClassInfo.cs" />
    <Compile Remove="XPDictionaryExtensions.cs" />
    <Compile Remove="XPODataType.cs" />
    <Compile Remove="Collections\DBCollection.cs" />
    <Compile Remove="DB\_ObjectMerger.cs" />
  </ItemGroup>
</Project>